{

  "manifest_version": 2,
  "name": "Controlador",
  "version": "1.0",

  "description": "Controla a pagina",

  "icons": {
    "48": "One-Piece-anime.ico"
  },

  "browser_action":{
      "default_popup":"index.html"
  },

  "content_scripts": [
    {
      "matches": ["*://*.politz.com.br/*"],
      "js": ["main.js"]
    }
  ]

}